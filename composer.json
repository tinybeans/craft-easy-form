{
    "name": "tinybeans/easy-form",
    "description": "You can create contact forms easily.",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "info@tinybeans.dev",
        "issues": "https://github.com/tinybeans/craft-easy-form/issues?state=open",
        "source": "https://github.com/tinybeans/craft-easy-form",
        "docs": "https://github.com/tinybeans/craft-easy-form",
        "rss": "https://github.com/tinybeans/craft-easy-form/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.5"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "tinybeans\\easyform\\": "src/"
        }
    },
    "extra": {
        "handle": "easy-form",
        "name": "Easy Form",
        "developer": "tinybeans",
        "documentationUrl": "https://github.com/tinybeans/craft-easy-form"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

